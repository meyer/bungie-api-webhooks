{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types"
    ],
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "noUncheckedIndexedAccess": true,
    "allowSyntheticDefaultImports": true
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
